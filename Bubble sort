#include<stdio.h>
void bubblesort(int x[],int n)
{int pass,i,temp;for(pass=1;pass<n;pass++){for(i=0;i<n-pass;i++){if(x[i]>x[i+1]){temp=x[i];x[i]=x[i+1];x[i+1]=temp;}}}}void main(){int i,n,x[10];void bubblesort();printf("\nHow Many Values:");scanf("%d",&n);printf("\nEnter Actual Values:\n");for(i=0;i<n;i++){scanf("%d",&x[i]);}bubblesort(x,n);printf("\nThe Sorted Elements Are:\n");for(i=0;i<n;i++){printf("%d\t",x[i]);}} /* output->
