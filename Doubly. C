
Doubly.c
#include<stdio.h>
#include<stdlib.h>
#include"doubly.h"
#include<malloc.h>
void main()
{
int ch,q=0;do
{
printf("\n1.create\n2.display\n3.insert\n4.Delete\n5.search\n6.exit\n");
printf("Enter the choice : ");
scanf("%d",&ch);switch(ch)
{
case 1: create(); 
break;
case 2: display(); 
break;
case 3: insert();
break;
case 4: delet();
break; 
case 5: search(); 
break;
case 6: q=1; 
break;
}
} 
while(q==0);
}
